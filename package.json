{
  "name": "@halkeye/jenkins-io-react",
  "version": "0.0.4",
  "description": "Random parts shared between jenkins.io",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "ts-node src/serve.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "npm run clean && node build.mjs",
    "clean": "rimraf dist",
    "lint-staged": "lint-staged",
    "lint": "eslint --ext .tsx,.ts -c .eslintrc.js"
  },
  "keywords": [
    "jenkins-infra",
    "react"
  ],
  "author": "Gavin Mogan <npm@gavinmogan.com> (https://www.gavinmogan.com/)",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.1",
    "@types/live-server": "^1.2.1",
    "@types/react-timeago": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "chokidar": "^3.5.3",
    "esbuild": "^0.14.49",
    "esbuild-node-externals": "^1.5.0",
    "eslint": "^8.23.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "live-server": "^1.2.2",
    "np": "^7.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "cheerio": "^1.0.0-rc.12",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-timeago": "^7.1.0",
    "style-to-object": "^0.3.0"
  },
  "peerDependencies": {
    "gatsby": "^4.18.0"
  },
  "files": [
    "dist/*",
    "makeLayout.js"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint --"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "jsx"
    ],
    "resetMocks": true
  }
}
