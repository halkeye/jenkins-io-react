{
  "name": "@halkeye/jenkins-io-react",
  "version": "0.0.4",
  "description": "Random parts shared between jenkins.io",
  "publishConfig": {
    "access": "public"
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "ts-node src/serve.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rimraf lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "prepack": "npm run build",
    "lint-staged": "lint-staged",
    "lint": "eslint --ext .tsx,.ts -c .eslintrc.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "keywords": [
    "jenkins-infra",
    "react"
  ],
  "author": "Gavin Mogan <npm@gavinmogan.com> (https://www.gavinmogan.com/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@storybook/addon-actions": "^6.5.11",
    "@storybook/addon-essentials": "^6.5.11",
    "@storybook/addon-interactions": "^6.5.11",
    "@storybook/addon-links": "^6.5.11",
    "@storybook/builder-webpack4": "^6.5.11",
    "@storybook/manager-webpack4": "^6.5.11",
    "@storybook/react": "^6.5.11",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.1",
    "@types/live-server": "^1.2.1",
    "@types/react-timeago": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "babel-loader": "^8.2.5",
    "chokidar": "^3.5.3",
    "esbuild": "^0.14.49",
    "esbuild-node-externals": "^1.5.0",
    "eslint": "^8.23.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-storybook": "^0.6.4",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "live-server": "^1.2.2",
    "np": "^7.6.2",
    "rimraf": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "cheerio": "^1.0.0-rc.12",
    "dedent-js": "^1.0.1",
    "prop-types": "^15.8.1",
    "react-timeago": "^7.1.0",
    "style-to-object": "^0.3.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "files": [
    "lib/**/*",
    "makeLayout.js"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint --"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "jsx"
    ],
    "resetMocks": true,
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": "./tsconfig.esm.json"
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/halkeye/jenkins-io-react.git"
  }
}
